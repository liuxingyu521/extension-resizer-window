{
  "manifest_version": 3,
  "name": "Window Resizer",
  "version": "1.0",
  "description": "Resize browser windows to preset device dimensions or custom sizes",
  "permissions": ["windows"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}